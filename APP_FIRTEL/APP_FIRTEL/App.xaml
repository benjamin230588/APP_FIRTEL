<?xml version="1.0" encoding="utf-8" ?>
<Application xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="APP_FIRTEL.App">
    <Application.Resources>
        <ResourceDictionary x:Name="dictionary">
            <OnPlatform x:TypeArguments="x:String" x:Key="Pacifico">
                <On Platform="Android" Value="Pacifico-Regular.ttf#Pacifico-Regular"/>
            </OnPlatform>
            <Style x:Key="btnloginfi23" TargetType="Button">
                <Setter Property="BackgroundColor" Value="Black" />
                <Setter Property="TextColor" Value="White" />

            </Style>
        </ResourceDictionary>

    </Application.Resources>
</Application>